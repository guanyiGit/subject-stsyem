package com.system.entity.testPaperLib;

//创建试卷设置题目分值列表对象
public class QuestionSetScore {
    private Integer subjectId;//题目id

    private Integer sunjectType;//题目类型0.单选  1.多选

    private Integer subjectMode;//题目分类

    private String subjectContent;//题目内容

    private Integer subjectDifficulty;//题目难度

    private float score;//题目分值

    public Integer getSubjectId() {
        return subjectId;
    }

    public void setSubjectId(Integer subjectId) {
        this.subjectId = subjectId;
    }

    public Integer getSunjectType() {
        return sunjectType;
    }

    public void setSunjectType(Integer sunjectType) {
        this.sunjectType = sunjectType;
    }

    public Integer getSubjectMode() {
        return subjectMode;
    }

    public void setSubjectMode(Integer subjectMode) {
        this.subjectMode = subjectMode;
    }

    public String getSubjectContent() {
        return subjectContent;
    }

    public void setSubjectContent(String subjectContent) {
        this.subjectContent = subjectContent;
    }

    public Integer getSubjectDifficulty() {
        return subjectDifficulty;
    }

    public void setSubjectDifficulty(Integer subjectDifficulty) {
        this.subjectDifficulty = subjectDifficulty;
    }

    public float getScore() {
        return score;
    }

    public void setScore(float score) {
        this.score = score;
    }
}
