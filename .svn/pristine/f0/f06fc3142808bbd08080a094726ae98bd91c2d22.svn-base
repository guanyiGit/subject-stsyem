package com.system.pojo.rt;

import java.util.List;

public class SubScore {
    /**
     * 题目序号
     */
    private Integer optSeq;
    /**
     * 题目id
     */
    private Integer subjectId;
    /**
     * 题目内容
     */
    private String content;

    /**
     * 题目图片url
     */
    private String subjectUrl;

    /**
     * 考生的选择  0.A ,1.B, 2.C, 3.D
     */
    private Integer myOptVal;
    /**
     * 考生做题结果  0.正确 1.错误
     */
    private Integer result;

    /**
     * 答案id
     */
    private Integer answerId;

    /**
     * 选项
     */
    private List<SubScoreOpts> subScoreOpts;


    public Integer getOptSeq() {
        return optSeq;
    }

    public void setOptSeq(Integer optSeq) {
        this.optSeq = optSeq;
    }

    public Integer getSubjectId() {
        return subjectId;
    }

    public void setSubjectId(Integer subjectId) {
        this.subjectId = subjectId;
    }

    public String getContent() {
        return content;
    }

    public void setContent(String content) {
        this.content = content;
    }

    public Integer getMyOptVal() {
        return myOptVal;
    }

    public void setMyOptVal(Integer myOptVal) {
        this.myOptVal = myOptVal;
    }

    public Integer getResult() {
        return result;
    }

    public Integer getAnswerId() {
        return answerId;
    }

    public void setAnswerId(Integer answerId) {
        this.answerId = answerId;
    }

    public String getSubjectUrl() {
        return subjectUrl;
    }

    public void setSubjectUrl(String subjectUrl) {
        this.subjectUrl = subjectUrl;
    }

    public void setResult(Integer result) {
        this.result = result;
    }

    public List<SubScoreOpts> getSubScoreOpts() {
        return subScoreOpts;
    }

    public void setSubScoreOpts(List<SubScoreOpts> subScoreOpts) {
        this.subScoreOpts = subScoreOpts;
    }
}
